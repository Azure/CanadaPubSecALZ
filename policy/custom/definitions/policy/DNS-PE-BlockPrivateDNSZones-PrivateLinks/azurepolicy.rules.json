{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.Network/privateDnsZones"
      },
      {
        "field": "name",
        "notLike": "*.privatelink.canadacentral.azmk8s.io"
      },
      {
        "field": "name",
        "notLike": "*.privatelink.canadaeast.azmk8s.io"
      },
      {
        "field": "name",
        "contains": "privatelink."
      }
    ]
  },
  "then": {
    "effect": "Deny"
  }
}