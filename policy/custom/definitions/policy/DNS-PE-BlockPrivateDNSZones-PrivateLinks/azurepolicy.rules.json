{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.Network/privateDnsZones"
      },
      {
        "anyOf": [
          {
            "field": "name",
            "like": "privatelink*"
          },
          {
            "field": "name",
            "like": "*privatelink"
          }
        ]
      }
    ]
  },
  "then": {
    "effect": "Deny"
  }
}